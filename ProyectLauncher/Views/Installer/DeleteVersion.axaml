<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="350" d:DesignHeight="420"
        CanResize="False"
        ExtendClientAreaToDecorationsHint="True"
        WindowStartupLocation="CenterOwner"
        ShowInTaskbar="False"
        Width="350" Height="420"
        x:Class="ProyectLauncher.Views.Installer.DeleteVersion"
        Title="Eliminar Versiones">
  
  <StackPanel HorizontalAlignment="Center" Margin="30">
    <TextBlock FontSize="20" FontWeight="Bold">Selecciona la Version a borrar</TextBlock>
    <ListBox Margin="10" Name="ListVersions" Height="300"/>
    <Button Name="DeleteBt" Click="Delete" Width="300" HorizontalAlignment="Center" HorizontalContentAlignment="Center">Eliminar Version</Button>
  </StackPanel>
  
</Window>
